{
  "name": "server",
  "version": "1.0.0",
  "description": "backend server for monta chatgpt",
  "main": "src/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node src/index.js",
    "dev": "nodemon src/index.js"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "js,json",
    "exec": "node src/index.js",
    "legacyWatch": true,
    "restartable": "rs",
    "ignore": [
      "*.test.js",
      "*.spec.js",
      ".git",
      "node_modules/"
    ]
  },
  "keywords": [
    "chatgpt"
  ],
  "author": "Mohamed Hashem",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "helmet": "^7.1.0",
    "openai": "^4.50.0",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "nodemon": "^3.1.3"
  }
}
